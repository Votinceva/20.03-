#include <stdio.h>
#include <stdlib.h>
#include <time.h>
#include <limits.h>
#define M 3 /*строк*/
#define N 4 /*столбцов*/

int main()
{
  int a[M][N] = {{2, 3, 3, 7}, {11, 22, 23, 14}, {21, 21, 23, 21}};
  int i, j, h, l = 0;

  for(i = 0; i < M; i++){
      for(j = 0; j < N; j++)
        printf("%3d", a[i][j]);
      printf("\n");

  }

  for(j = 0; j < N; j++){
   if (l == 0){
    for(i = 0; i < M; i++){
            if (a[i][j]%2 == 1){
                l++;
                h = j;
            }
    }
   }
   else break;
    if (l != M) l = 0;
  }
    if (l != 0){
        printf("%d\n", h);
    }
    else printf("ops\n");

  return 0;
}
