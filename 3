#include <stdio.h>
#include <stdlib.h>
#include <time.h>
#include <limits.h>
#define M 3 /*строк*/
#define N 4 /*столбцов*/

int main()
{
  int a[M][N] = {{1, 2, 3, 4}, {11, 22, 23, 14}, {22, 22, 23, 24}};
  int o[M];
  int i, j, k;
  int oda = 0, t = 0, yuy = 0;


 for(i = 0; i < M; i++){
      for(j = 0; j < N; j++)
        printf("%5d", a[i][j]);
      printf("\n");

  }

   for(i = 0; i < M; i++){
    for(j = 0; j < N; j++){
        for(k = 1; k < N; k++)
            if ((j+k) < N){
                if (a[i][j] == a[i][j+k])
                    oda++;

                    yuy++;
            }
            else break;
    }
    o[i] = oda;
    oda = 0;

  }
if (yuy != 0){
  for(i = 1; i < M; i++)
    printf ("%d %d\n", o[i], o[i-1]);
    if (o[i-1] << o[i]){
        t = i-1;
        printf ("%d\n", t);
    }
printf ("%d\n", t);
}
else printf ("netyu:(");

  return 0;
}
